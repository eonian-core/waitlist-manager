{
  "name": "@eonian/waitlist-manager",
  "private": true,
  "scripts": {
    "dev": "concurrently -c \"auto\" \"yarn:build:watch\" \"yarn:start:watch\"",
    "build": "tsc",
    "build:watch": "tsc -w",
    "start": "node build/server.js",
    "start:watch": "nodemon build/server.js",
    "deploy": "fly deploy",
    "run:worker": "node build/worker.js",
    "test": "jest"
  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.7.0",
    "concurrently": "^8.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "dockerfile": {
    "cache": ""
  }
}
